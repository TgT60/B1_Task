@model ExcelModel

<form method="post" enctype="multipart/form-data">
    <div class="row">
        <label>Select File</label>
        <input type="file" name="file" />
    </div>
    <div class="row" style="padding-top: 10px;">
        <div class="col-3">
            <input type="submit" asp-controller="Excel" asp-action="ExcelFileReader" class="btn btn-dark" value="Read Excel File" />
        </div>
    </div>
</form>




@if (Model != null && Model.FlatData != null)
{
    <h2>Flat Data</h2>
    <ul>
        @foreach (var item in Model.FlatData)
        {
            <li>@item</li>
        }
    </ul>
}
else
{
    <p>No data available.</p>
}